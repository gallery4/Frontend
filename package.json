{
	"name": "gallery-frontend",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"preview-host": "vite preview --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"generate-grpc": "protoc --ts_out=src/lib/grpc --proto_path=src/lib/grpc/protos browse.proto download.proto image.proto types.proto"
	},
	"devDependencies": {
		"@grpc/grpc-js": "^1.13.4",
		"@protobuf-ts/grpc-transport": "^2.11.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/eslint": "^9.6.0",
		"@types/google-protobuf": "^3.15.12",
		"@types/node": "^22.10.1",
		"@types/path-browserify": "^1.0.3",
		"daisyui": "^5.0.50",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.2.4",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"dependencies": {
		"@macfja/svelte-persistent-store": "^2.4.2",
		"@protobuf-ts/plugin": "^2.11.1",
		"@react2svelte/swipeable": "^0.1.4",
		"@sveltejs/adapter-node": "^5.2.9",
		"@sveltestrap/sveltestrap": "^6.2.7",
		"@types/mime-types": "^2.1.4",
		"@types/natural-compare-lite": "^1.4.2",
		"@types/string-hash": "^1.1.3",
		"dotenv": "^16.4.5",
		"google-protobuf": "^3.21.4",
		"mime-types": "^2.1.35",
		"natural-compare-lite": "^1.4.0",
		"path-browserify": "^1.0.1",
		"pdfjs-dist": "^4.10.38",
		"string-hash": "^1.1.3",
		"svelte-reader": "^1.1.0",
		"svelte-tippy": "^1.3.2",
		"tippy.js": "^6.3.7",
		"vidstack": "^1.12.12"
	}
}
