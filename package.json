		"hammerjs": "^2.0.8",
		"mime-types": "^3.0.1",
		"natural-compare-lite": "^1.4.0",
		"path-browserify": "^1.0.1",
		"pdfjs-dist": "^5.4.54",
		"string-hash": "^1.1.3",
		"svelte-icon": "^2.1.1",
		"ts-enum-util": "^4.1.0",
		"vidstack": "^1.12.13"
	}
